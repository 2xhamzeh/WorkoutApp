{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "forceConsistentCasingInFileNames": true,
    },
    "include": ["src/**/*", "types/**/*", "src/__tests__/jest.setup.ts"],
  }
  